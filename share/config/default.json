{
  "app": {
    "name": "Prometheus2",
    "description": "Default SunsetCoders project",
    "debug": true,
    "required_php_modules" : ["curl","json","ldap","libxml","mysqli","xsl" ]
  },
  "log": {
    "class": "Prometheus2\\common\\logging\\dblogger",
    "table": "prom2_log"
  },
  "db": {
    "host": "localhost",
    "user": "root",
    "pass": "",
    "catalogue": "prom2",
    "port": 3306,
    "socket": 0
  },
  "god": {
    "host": "localhost",
    "user": "root",
    "pass": "",
    "catalogue": "prom2",
    "port": 3306,
    "socket": 0
  },
  "security" : {
    "session_regenerate_mins" : 10
  },
  "format" : {
    "endiandate" : "d/m/Y",
    "prettydate" : "l jS \\of F Y h:i:s A"
  }
}
